<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>fr.liglab.adele.cilia</groupId>
	<artifactId>cilia-project</artifactId>
	<packaging>pom</packaging>
	<name>Cilia Project</name>
	<version>1.2.2-SNAPSHOT</version>
	<url>http://cilia.ligforge.imag.fr</url>
	<properties>
		<skipTests>true</skipTests>
	</properties>
        <scm>
    		<connection>scm:svn:svn+ssh://cilia@svn.ligforge.imag.fr/svnroot/cilia/trunk</connection>
		<developerConnection>scm:svn:svn+ssh://cilia@svn.ligforge.imag.fr/svnroot/cilia/trunk</developerConnection>
		<url>https://svn.ligforge.imag.fr/svnroot/cilia/</url>
	</scm>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
			        <artifactId>maven-release-plugin</artifactId>
		        	<configuration>
			          <!--tagBase>https://svn.ligforge.imag.fr/svnroot/cilia/tags/releases</tagBase-->
				  <allowTimestampedSnapshots>true</allowTimestampedSnapshots> 
			        </configuration>
			</plugin>
			<plugin>
			        <groupId>org.apache.maven.plugins</groupId>
			        <artifactId>maven-surefire-plugin</artifactId>
			        <version>2.12</version>
			        <configuration>
			          <skipTests>${skipTests}</skipTests>
			        </configuration>
			</plugin>
		    </plugins>
	</build>

