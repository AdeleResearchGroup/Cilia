<ipojo>
	<!-- Gogo shell interface -->
	<component classname="fr.liglab.adele.cilia.knowledge.command.shell.GogoCommands"
		name="ModelRuntime_Compendium.Gogo">
		<provides specifications="fr.liglab.adele.cilia.knowledge.command.shell.GogoCommands">
			<property name="osgi.command.scope" type="String" value="cilia.runtime" />
			<property name="osgi.command.function" type="String[]"
				value="{
				        reg_find,
				        node_connected_to,node_dump,node_setup,node_rawdata,
				        cache_size,cache_dump,cache_clear,
				        app_chain_list,app_endpoints_in,app_endpoints_out,app_connected_to,
				        imported_services,
				        my_entry
				        }" />
		</provides>
		<requires field="exec" />
		<requires field="eventbus" />
		<requires field="specification" />
	</component>
	<instance component="ModelRuntime_Compendium.Gogo" />
</ipojo>